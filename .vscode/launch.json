    "version": "0.2.0",
    "configurations": [
        {
            "name": "Flutter Web (Debug)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": ["--web-renderer", "auto"],
            "flutterMode": "debug",
            "cwd": "${workspaceFolder}",
            "env": {
                "CHROME_FLAGS": "--disable-web-security"
            }
        },
        {
            "name": "new_table",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "new_table (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "new_table (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}